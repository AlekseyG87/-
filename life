#include <iostream>
#include <cstdlib>
#include <fstream>
using namespace std;

int main()
{
    ifstream cin("input.txt");
    ofstream cout("output.txt");
    int n,m,k;
    int a[n][m];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            a[i][j]=rand()%2;
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }
    for(int b=0;b<k;b++)
    {
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            if(a[i][j]==0)
            {
             if(a[i][j%n]+a[i%n][j]+a[i][(j+2)%n]+a[(i+2)%n][j]==3)
             {
                 a[i][j]=0;
             }
            }
            if(a[i][j]==1)
            {
                if(a[i][j%n]+a[i%n][j]+a[i][(j+2)%n]+a[(i+2)%n][j]<2)
                {
                    a[i][j]=0;
                }
                if(a[i][j%n]+a[i%n][j]+a[i][(j+2)%n]+a[(i+2)%n][j]>3)
                {
                    a[i][j]=0;
                }
                if(a[i][j%n]+a[i%n][j]+a[i][(j+2)%n]+a[(i+2)%n][j]==2)
                {
                    a[i][j]=1;
                }
                if(a[i][j%n]+a[i%n][j]+a[i][(j+2)%n]+a[(i+2)%n][j]==2)
                {
                    a[i][j]=1;
                }
            }
        }
    }
    }
    return 0;
}
